+    m_fromYear = fromYear;
+    m_toYear = toYear;
+    m_genres = genres;
+    m_currentPlayingPos = 0;
+    m_currentResultPos = 0;
+    m_currentSearch = NULL;
+    //hard code the number of tracks to 15 for now
+    m_numberOfTracksToDisplay = Settings::getInstance()->getRadioNumberTracks();
