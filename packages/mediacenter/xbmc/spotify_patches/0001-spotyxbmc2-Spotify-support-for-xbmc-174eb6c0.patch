+
+   void Utils::updateSearchResults(string query) {
+     //we need to replace the whitespaces with %20
+
+     int pos = 0;
+     while (pos != string::npos) {
+       pos = query.find(' ');
+       if (pos != string::npos) {
+         query.replace(pos, 1, "%20");
+       }
+     }
+
+     Logger::printOut("updating search results");
+     CStdString path;
+     path.Format("musicsearch://%s/", query);
+     updatePath(path);
+   }
